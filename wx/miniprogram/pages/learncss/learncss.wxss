/* Cascading Style Sheets */
page {
    height: 100%;
}

.item {
    font-size: xx-large;
}

.list {
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-around;
    height: 750rpx;
    align-self: stretch;
}

.blue {
    color: blue;
}

.red {
    color: red;
}